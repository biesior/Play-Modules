web: target/start -Dhttp.port=${PORT} ${JAVA_OPTS} -DapplyEvolutions.default=true -Ddb.default.driver=org.postgresql.Driver -Ddisqus.forum=$disqusForum -Dgithub.clientId=$githubClientId -Dgithub.secret=$githubSecret -Dgoogle.analytics=$googleAnalytics -Dtwitter.clientId=$twitterClientId -Dtwitter.secret=$twitterSecret  -Dsmtp.host=smtp.sendgrid.net -Dsmtp.user=SENDGRID_USERNAME -Dsmtp.password=SENDGRID_PASSWORD -Ddb.default.url=$DATABASE_URL


